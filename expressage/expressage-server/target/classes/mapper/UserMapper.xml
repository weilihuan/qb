<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.expressage.dao.UserDAO">

    <select id="queryUser" resultType="com.expressage.entity.User">
        select * from user
    </select>

    <select id="queryById" resultType="com.expressage.entity.User">
        select * from user where user_id=#{userId}
    </select>
</mapper>